{
  "name": "hakoniwa",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "all": "run-p watch:css watch:js",
    "css:scss:editor-style": "sass src/scss/admin/editor-style.scss dist/assets/css/editor-style.css --no-source-map",
    "css:postcss:editor-style": "postcss dist/assets/css/editor-style.css -o dist/assets/css/editor-style.css --no-map",
    "css:scss:admin": "sass src/scss/style-admin.scss dist/assets/css/admin.css --no-source-map",
    "css:postcss:admin": "postcss dist/assets/css/front-end.css -o dist/assets/css/front-end.css --no-map",
    "css:scss:front-end": "sass src/scss/style.scss dist/assets/css/front-end.css --no-source-map",
    "css:postcss:front-end": "postcss dist/assets/css/front-end.css -o dist/assets/css/front-end.css --no-map",
    "css:postcss:min:front-end": "postcss dist/assets/css/front-end.css -u cssnano -o dist/assets/css/front-end.min.css --no-map",
    "css:scss:back-end": "sass src/scss/style-back-end.scss dist/assets/css/back-end.css --no-source-map",
    "css:postcss:back-end": "postcss dist/assets/css/back-end.css -o dist/assets/css/back-end.css --no-map",
    "css:postcss:min:back-end": "postcss dist/assets/css/back-end.css -u cssnano -o dist/assets/css/back-end.min.css --no-map",
    "js:minify": "npx rollup -c",
    "watch:css:editor-style": "watch 'run-s css:scss:editor-style css:postcss:editor-style' ./src/scss",
    "watch:css:admin": "watch 'run-s css:scss:admin css:postcss:admin copy:css' ./src/scss",
    "watch:css:front-end": "watch 'run-s css:scss:front-end css:postcss:front-end css:postcss:min:front-end copy:css' ./src/scss",
    "watch:css:back-end": "watch 'run-s css:scss:back-end css:postcss:back-end css:postcss:min:back-end copy:css' ./src/scss",
    "watch:js": "watch 'run-s js:minify copy:js' ./src/js",
    "watch:css-all": "run-p watch:css:front-end watch:css:back-end watch:css:admin",
    "watch:tailwind": "npx tailwindcss -i ./src/scss/tailwind/input.scss -o ./src/scss/admin/_tailwind.scss --watch",
    "lint:js": "npx eslint './src/js/**/*.js'",
    "lint-fix:js": "npx eslint './src/js/**/*.js' --fix",
    "lint:scss": "npx stylelint './src/scss/**/*.scss'",
    "lint-fix:scss": "npx stylelint './src/scss/**/*.scss' --fix",
    "copy:css": "npx cpx './dist/assets/css/*.css' ./assets/css",
    "copy:css:blocks": "npx cpx './dist/assets/css/blocks/**' ./assets/css/blocks",
    "copy:js": "npx cpx './dist/assets/js/*.js' ./assets/js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "@wordpress/eslint-plugin": "^14.4.0",
    "@wordpress/scripts": "^23.6.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.14",
    "core-js": "^3.11.0",
    "cpx": "^1.5.0",
    "cssnano": "^5.0.1",
    "eslint": "^7.24.0",
    "eslint-loader": "^4.0.2",
    "glob": "^7.1.6",
    "js-beautify": "^1.13.13",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "postcss-cli": "^8.3.1",
    "postcss-combine-duplicated-selectors": "^10.0.2",
    "postcss-comment": "^2.0.0",
    "postcss-nested": "^5.0.5",
    "postcss-sorting": "^6.0.0",
    "rollup": "^2.45.2",
    "rollup-plugin-uglify": "^5.0.2",
    "sass": "^1.48.0",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "tailwindcss": "^3.2.2",
    "uglify-js": "^3.13.4",
    "watch": "^0.13.0",
    "webpack": "^5.35.0",
    "webpack-cli": "^4.6.0"
  }
}
